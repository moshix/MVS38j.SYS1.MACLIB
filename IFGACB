IFGACB   DSECT                                 ACCESS METHOD CONTROL    11750000
*                                              BLOCK                    11800000
         AGO   .ACBDSCT                                                 11850000
.ACBNODS ANOP                                                           11900000
IFGACB   DS    0D                              ACCESS METHOD CONTROL    11950000
*                                              BLOCK                    12000000
.ACBDSCT ANOP                                                           12050000
ACBID    DS    XL1                             ACB IDENTIFIER           12100000
ACBIDVAL EQU   X'A0'                           IDENTIFIER VALUE - X'A0' 12110000
ACBSTYP  DS    XL1                             ACB SUBTYPE              12150000
ACBSVSAM EQU   X'10'                           VSAM SUBTYPE     X04SVHS 12160000
ACBSVRP  EQU   X'11'                           VRP SUBTYPE      X04SVHS 12170000
ACBSVTAM EQU   X'20'                           VTAM SUBTYPE     X04SVHS 12180000
ACBS3540 EQU   X'40'                           3540 SUBTYPE     X04SVHS 12190000
ACBLENG  DS    0H                              ACB LENGTH IN BYTES      12200000
ACBLEN2  DS    0H                              ALTERNATE NAME FOR       12202000
*                                              ACBLENG           X03004 12204000
ACBLENG2 DS    H                               ALTERNATE NAME FOR       12210000
